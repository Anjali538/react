  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --config ./webpack.config.js --port 8080 --mode development --open --hot",
    "build-dev": "npm run lint && webpack --config ./webpack.config.js --progress -d -w && npm run deploy-local",
    "build-prod": "npm run lint && webpack --config ./webpack.config.js --progress -p",
    "analyze": "webpack-bundle-analyzer --port 8888 --mode static  dist/stats.json",
    "lint": "eslint src/**/*.js --ext src/**/*.css --ext src/**/*.png --ext src/**/*.otf --fix",
    "deploy-local": "http-server -p 7000 dist/",
    "deploy-prod": "http-server NODE_ENV=production -p 8000 dist/"
  },
